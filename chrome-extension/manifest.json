
{
  "name": "Earth Engine Connector",
  "description": "Load local NodeJS scripts into Earth Engine",
  "version": "0.0.2",
    "default_locale": "en",
  "permissions": [
    "activeTab",
     "http://127.0.0.1:9812/"
  ],
   "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://code.earthengine.google.com/*"],
      "js": [
        "scripts/main.js"
        ]
    }
  ],
  "web_accessible_resources" : [
    "scripts/build.js",
    "scripts/build.html",
    "images/icon-128.png"
  ],
  "manifest_version": 2
}